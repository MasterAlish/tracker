<h1>Users </h1>

<table>
  <thead>
    <tr>
        <th>Name</th>
        <th>E-mail</th>
        <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    <%@users.each do |user| %>
        <tr>
          <td><%=user.name %></td>
          <td><%=user.email %></td>
          <td><%=link_to "Delete", user, method: :delete%></td>
        </tr>
    <% end %>
  </tbody>
</table>